@{
  ViewData["Title"] = $"Delete {Model.CityName}";
}

@model TravelClient.Models.Destination

<h4>Are you sure you want to delete this review for @Model.CityName</h4>

@using (Html.BeginForm())
{
  <input type="submit" value="Delete"/>
}

@Html.ActionLink("Back to details", "Details", new { id = @Model.DestinationId})